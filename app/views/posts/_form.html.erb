
<%= form_with model: @post do |f| %>
<div class="form-group ">
	<%=f.label :title%>
  <%=f.text_field :title, class: "form-control"%>
</div>
 <div class="form-group">
  <%=f.label :body%>
  <%=f.text_area :body, class: "form-control"%>
</div>
  <div class="form-group">
  	<%=f.submit "create post", class: "btn btn-primary"%>
  </div>
  
<% end %>